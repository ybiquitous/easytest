module Easytest
  class Case
    attr_reader file: String

    attr_reader block: (^() -> void)?

    attr_reader skipped: bool

    attr_reader only: bool

    alias skipped? skipped

    alias only? only

    def initialize: (name: String, ?skipped: bool, ?only: bool) ?{ () -> void } -> void

    def skip!: () -> void

    def run: () -> [Symbol, String?]
  end
end
